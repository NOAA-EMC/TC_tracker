   2456 c
   2457 c    &             ,' str=',kstream,' ver=',kver,' mem=',kfnum
   2458 c      print '(7(a6,i6))','  lu=',ludn,' cls=',kclass,' typ=',ktype
   2459 c     &             ,' str=',kstream,' mem=',kfnum
   2460 c     &             ,' tot=',ktot,' b52=',junk
   2461       goto 333
   2462 C
   2463  333  CONTINUE
   2464       RETURN
   2465       END

